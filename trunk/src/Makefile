force-reedit:
		cd $(top_srcdir) && \
	  $(AUTOMAKE) --gnu  src/Makefile
	cd $(top_srcdir) && perl admin/am_edit src/Makefile.in


#>+ 21
clean-bcheck: 
	rm -f *.bchecktest.cc *.bchecktest.cc.class a.out

bcheck: bcheck-am

bcheck-am:
	@for i in ; do \
	    if test $(srcdir)/$$i -nt $$i.bchecktest.cc; then \
	        echo "int main() {return 0;}" > $$i.bchecktest.cc ; \
	        echo "#include \"$$i\"" >> $$i.bchecktest.cc ; \
	        echo "$$i"; \
	        if ! $(CXXCOMPILE)  --dump-class-hierarchy -c $$i.bchecktest.cc; then \
	            rm -f $$i.bchecktest.cc; exit 1; \
	        fi ; \
	        echo "" >> $$i.bchecktest.cc.class; \
	        perl $(top_srcdir)/admin/bcheck.pl $$i.bchecktest.cc.class || { rm -f $$i.bchecktest.cc; exit 1; }; \
	        rm -f a.out; \
	    fi ; \
	done


#>+ 11
kiptablesgenerator.all_cpp.cpp: $(srcdir)/Makefile.in $(srcdir)/main.cpp $(srcdir)/kiptablesgenerator.cpp $(srcdir)/rulesdialog.cpp  kiptablesgenerator.moc rulesdialog.moc
	@echo 'creating kiptablesgenerator.all_cpp.cpp ...'; \
	rm -f kiptablesgenerator.all_cpp.files kiptablesgenerator.all_cpp.final; \
	echo "#define KDE_USE_FINAL 1" >> kiptablesgenerator.all_cpp.final; \
	for file in main.cpp kiptablesgenerator.cpp rulesdialog.cpp ; do \
	  echo "#include \"$$file\"" >> kiptablesgenerator.all_cpp.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> kiptablesgenerator.all_cpp.final; \
	done; \
	cat kiptablesgenerator.all_cpp.final kiptablesgenerator.all_cpp.files > kiptablesgenerator.all_cpp.cpp; \
	rm -f kiptablesgenerator.all_cpp.final kiptablesgenerator.all_cpp.files

#>+ 3
